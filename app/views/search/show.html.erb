<h1>Search results for <%= @query %></h1>
<h2>Questions</h2>

<ul>
<% @questions.each do |question| %>
    <li><%= link_to question.heading, question_path(question) %></li>
<% end %>
</ul>

<h2>Users</h2>

<ul>
<% @users.each do |user| %>
    <li><%= link_to user.username, user_path(user) %></li>
<% end %>
</ul>
